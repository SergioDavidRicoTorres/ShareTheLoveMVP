{
  "expo": {
    "scheme": "sharethelovemvp",
    "name": "ShareTheLoveMVP",
    "slug": "ShareTheLoveMVP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#01042B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sergiodavidricotorres.ShareTheLoveMVP"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sergiodavidricotorres.ShareTheLoveMVP",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e49a9ea5-4a8c-4c93-9c67-66408c04e811"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Select and upload images",
          "cameraPermission": "Allow Share The Love to access your camera."
        }
      ]
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e49a9ea5-4a8c-4c93-9c67-66408c04e811"
    }
  }
}
